{
  "name": "react-starter",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "react",
    "redux",
    "saga"
  ],
  "author": "Hugo Nogueira <hugom@gmail.comn>",
  "license": "UNLICENSED",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/tests/**"
    ]
  },
  "scripts": {
    "start": "REACT_APP_ENV=development sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts start'",
    "build": "REACT_APP_ENV=production sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts build'",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false --testPathIgnorePatterns=/src/tests/",
    "test:staged": "CI=true react-scripts test --env=jsdom --findRelatedTests",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "eject": "react-scripts eject",
    "cypress:run": "node_modules/.bin/cypress run --project ./src/tests/e2e",
    "cypress:open": "node_modules/.bin/cypress open --project ./src/tests/e2e",
    "cypress:verify": "node_modules/.bin/cypress verify",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix"
  },
  "dependencies": {
    "@sentry/browser": "5.6.2",
    "amazon-cognito-identity-js": "3.0.13",
    "axios": "0.19.0",
    "camelcase": "5.3.1",
    "connected-react-router": "6.5.2",
    "core-js": "3.2.1",
    "formik": "1.5.8",
    "grommet": "2.7.6",
    "grommet-icons": "4.3.0",
    "grommet-styles": "0.2.0",
    "history": "4.9.0",
    "i18next": "17.0.6",
    "immer": "3.2.0",
    "lint-staged": "9.2.3",
    "marked": "0.7.0",
    "moment": "2.24.0",
    "polished": "3.4.1",
    "react": "16.9.0",
    "react-app-polyfill": "1.0.2",
    "react-currency-format": "1.0.0",
    "react-dom": "16.9.0",
    "react-i18next": "10.11.4",
    "react-moment": "0.9.2",
    "react-redux": "7.1.0",
    "react-redux-loading-bar": "4.4.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-toastify": "5.3.2",
    "react-toastify-redux": "1.0.0-rc.2",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.0.5",
    "redux-sentry-middleware": "0.1.1",
    "styled-components": "4.3.2",
    "typesafe-actions": "4.4.2",
    "validator": "11.1.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@commitlint/prompt": "8.1.0",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-console": "1.2.1",
    "@storybook/addon-links": "5.1.11",
    "@storybook/addons": "5.1.11",
    "@storybook/react": "5.1.11",
    "@testing-library/dom": "6.1.0",
    "@testing-library/jest-dom": "4.0.1",
    "@testing-library/react": "9.1.3",
    "@testing-library/react-hooks": "2.0.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/history": "4.7.2",
    "@types/jest": "24.0.18",
    "@types/marked": "0.6.5",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.4",
    "@types/react-test-renderer": "16.9.0",
    "@types/redux-mock-store": "1.0.1",
    "@types/redux-sentry-middleware": "0.1.1",
    "@types/styled-components": "4.1.18",
    "@types/yup": "0.26.23",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "acorn": "7.0.0",
    "ajv": "6.12.3",
    "babel-loader": "8.0.5",
    "commitizen": "4.0.3",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "3.0.4",
    "jest-localstorage-mock": "2.4.0",
    "jest-styled-components": "6.3.3",
    "log-symbols": "3.0.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "1.5.3",
    "redux-saga-test-plan": "4.0.0-beta.2",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-xor": "1.0.8",
    "typescript": "3.4.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add",
      "npm run test:staged"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
